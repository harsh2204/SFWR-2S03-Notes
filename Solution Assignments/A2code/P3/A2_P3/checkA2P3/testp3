#!/bin/bash


make
echo " ----------------- N = 10, a = 0, b = 1, x = 0.5---------------"
./check 10 0 1 0.5
echo " ----------------- N = 21, a = 1.2, b = 3.01, x = 2.95---------------"
./check 21 1.2 3.01 2.95
echo " ----------------- N = 200, a = 3, b = 5, x = 3.06---------------"
./check 200 3 5 3.06
make clean


